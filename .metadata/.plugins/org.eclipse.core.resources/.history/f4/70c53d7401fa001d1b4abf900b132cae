class Animal :
    age = 0
    def oneYear(self):
        self.age += 1
        return self.age

class Human(Animal) :
    flagLang = False
    
    def momstouch(self):
        self.flagLang=True

a = Animal()
print(a.age)
a.oneYear()
print(a.age)

h = Human()
print(h.age)
h.oneYear()
print(h.age)
print(h.flagLang)
h.momstouch()
print(h.flagLang)
